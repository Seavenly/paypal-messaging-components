        "belter": "^1.0.112",
        "core-js-pure": "^3.0.1",
        "jsx-pragmatic": "^2.0.7",
        "post-robot": "^10.0.29",
        "zalgo-promise": "^1.0.38",
        "zoid": "^9.0.46"
    },
    "devDependencies": {
        "@testing-library/dom": "^6.2.0",
        "fs-extra": "^9.0.1",
        "globby": "^11.0.1",
